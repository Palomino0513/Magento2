{
    "name": "magento/project-enterprise-edition",
    "description": "eCommerce Platform for Growth (Enterprise Edition)",
    "type": "project",
    "version": "2.2.5",
    "license": [
        "proprietary"
    ],
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        "swissup": {
            "type": "composer",
            "url": "https://docs.swissuplabs.com/packages/"
        }
    },
    "require": {
        "magento/magento-cloud-metapackage": "2.2.5",
        "composer/composer": "@alpha",
        "magepal/magento2-reindex": "^1.0",
        "mageplaza/magento-2-social-login": "^2.6",
        "mageplaza/magento-2-blog-extension": "^3.0",
        "mailchimp/mc-magento2": "^1.0",
        "yotpo/module-review": "^2.4",
        "cweagans/composer-patches": "^1.6",
        "swissup/slick-carousel": "^1.2",
        "aftership/aftership-php-sdk": "^5.1",
        "postmen/sdk-php": "^1.0",
        "mageplaza/module-smtp": "^1.2",
        "conekta/conekta-php": "dev-master",
        "fastly/magento2": "^1.2",
        "magepal/magento2-cataloglazyload": "^1.6",
        "msp/devtools": "^1.2",
        "facebook/php-business-sdk": "^9.0",
        "chillerlan/php-qrcode": "^2.0",
        "vlucas/pikirasa": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2.0",
        "squizlabs/php_codesniffer": "3.2.2",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "2.5.2",
        "sjparkinson/static-review": "~4.1",
        "fabpot/php-cs-fixer": "~2.1.1",
        "lusitanian/oauth": "~0.8.10",
        "sebastian/phpcpd": "2.0.4"
    },
    "config": {
        "use-include-path": true,
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/"
        },
        "psr-0": {
            "": "app/code/"
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/",
            "Magento\\ToolkitFramework\\": "dev/tools/performance-toolkit/framework/Magento/ToolkitFramework/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "magento-force": "override",
        "composer-exit-on-patch-failure": true,
        "patches": {
            "magento/module-catalog": {
                "Fixed add to wishlist issue on product price 0": "patches/composer/github-fixwishlist.diff"
            },
          "magento/module-customer": {
            "Fixed create account after success shopping": "patches/composer/github-fixcreateacountsuccess.diff"
          },
          "magento/module-email": {
            "Fix error not save new theme": "patches/composer/github-fixsaveapplytheme.diff"
          }
        }
    }
}
